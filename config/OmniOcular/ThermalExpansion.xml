<oo>
	<init>
		function gtteUT(n, cap){
			if(nbt['Empty']==undefined){
				return fluidName(n['FluidName'])+TAB+WHITE+n['Amount']+" / "+cap+" mB"}
		}
	</init>
	<init>
		function gtteS(n, cap){
			if(nbt['FluidName']!=undefined){
				return fluidName(n['FluidName'])+TAB+WHITE+n['Amount']+" / "+cap+" mB"}
		}
	</init>
	<init>
		function gtteWT(n, cap){
